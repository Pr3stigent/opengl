# Alternative GNU Make project makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild

ifeq ($(config),debug)
TARGETDIR = ../bin/Debug-windows-x86_64/PracticeProject
TARGET = $(TARGETDIR)/PracticeProject
  define BUILDCMDS
  endef
  define CLEANCMDS
  endef

else ifeq ($(config),release)
TARGETDIR = ../bin/Release-windows-x86_64/PracticeProject
TARGET = $(TARGETDIR)/PracticeProject
  define BUILDCMDS
	@echo Running build commands
	make Release
  endef
  define CLEANCMDS
	@echo Running clean commands
	make clean Release
  endef

else
  $(error "invalid configuration $(config)")
endif

$(TARGET):
	$(BUILDCMDS)

clean:
	$(CLEANCMDS)
